{
  "name": "expo-docs",
  "version": "38.0.0",
  "private": true,
  "scripts": {
    "dev": "rimraf node_modules/.cache && node server.js",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "export": "yarn run build && next export",
    "export-server": "http-server out -p 8000",
    "import-react-native-docs": "node ./scripts/import-react-native-docs.js",
    "test-links": "node --async-stack-traces --unhandled-rejections=strict ./scripts/test-links.js",
    "prettier": "prettier --write '**/*.{js,ts,tsx}'",
    "test": "jest",
    "schema-sync": "node --async-stack-traces --unhandled-rejections=strict ./scripts/schema-sync.js"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@mdx-js/loader": "^0.16.8",
    "@mdx-js/mdx": "^0.16.8",
    "@mdx-js/runtime": "^1.6.6",
    "@reach/tabs": "^0.10.3",
    "@sentry/browser": "^5.6.1",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-preval": "^3.0.1",
    "cross-env": "^5.2.0",
    "docsearch.js": "^2.5.2",
    "emotion": "^9.2.9",
    "emotion-server": "^9.2.9",
    "front-matter": "^2.3.0",
    "fs-extra": "^6.0.1",
    "github-slugger": "^1.2.0",
    "hotshot": "^1.0.5",
    "next": "^8.0.0",
    "nprogress": "0.2.0",
    "prism-react-renderer": "1.0.2",
    "prop-types": "15.6.2",
    "react": "^16.8.1",
    "react-diff-view": "^2.4.4",
    "react-dom": "^16.8.1",
    "react-emotion": "^9.2.9",
    "react-player": "^1.11.1",
    "react-visibility-sensor": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "axios": "^0.19.2",
    "danger": "^9.2.10",
    "http-server": "^0.12.3",
    "jest": "26.1.0",
    "prettier": "^1.18.2",
    "puppeteer": "^3.3.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2"
  }
}
