{
  "name": "storybook",
  "version": "1.0.0",
  "description": "Storybook for the Expo SDK",
  "main": "__generated__/AppEntry.js",
  "sideEffects": [
    "/.storybook/config.js"
  ],
  "scripts": {
    "postinstall": "expo-yarn-workspaces postinstall",
    "start": "start-storybook --port 6006",
    "build": "build-storybook -c .storybook -o web-build/ --quiet",
    "test:web": "BABEL_ENV=test:web jest --config=./test/jest.config.web.js",
    "pretest:web:ci": "npm run build",
    "test:web:ci": "export IS_EXPO_CI=true; npm run test:web --ci",
    "update:test:web:ci": "rm -rf .storybook/__image_snapshots__ && npm run test:web:ci -u",
    "test:native": "BABEL_ENV=test:native jest --config=./test/jest.config.native.js",
    "deploy": "yarn build && netlify deploy --prod"
  },
  "author": "Expo",
  "license": "MIT",
  "powertools": {
    "group": "client"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@reach/router": "^1.2.1",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-backgrounds": "^5.0.11",
    "@storybook/addon-centered": "^5.0.11",
    "@storybook/addon-events": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-jest": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addon-options": "5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addon-storyshots-puppeteer": "^5.0.11",
    "@storybook/addons": "5.0.11",
    "@storybook/client-logger": "^5.0.11",
    "@storybook/core": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/router": "^5.0.11",
    "@storybook/theming": "^5.0.11",
    "@storybook/ui": "^5.0.11",
    "@types/jest": "^24.0.11",
    "@types/storybook__react": "^4.0.1",
    "expo-cli": "^2.17.3",
    "github-markdown-css": "^3.0.1",
    "marked": "^0.6.2",
    "memoizerific": "^1.11.3",
    "react": "16.6.0",
    "react-markdown": "^4.0.8",
    "react-native": "0.59.8",
    "react-native-svg": "~9.5.1",
    "react-native-table-component": "^1.2.0",
    "react-native-web": "^0.11.4",
    "react-syntax-highlighter": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@expo/webpack-config": "^0.1.4",
    "@types/expo": "^32.0.0",
    "@types/react-native": "^0.57.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "^3.3.5",
    "expo-yarn-workspaces": "^1.2.0",
    "jest": "^24.8.0",
    "node-fetch": "^2.0.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.4",
    "typescript": "^3.3.0",
    "url-loader": "^1.1.2",
    "webpack": "~4.8.1",
    "webpack-cli": "^3.3.0",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
