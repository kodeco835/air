#!/usr/bin/env node
'use strict';

const childProcess = require('child_process');
const path = require('path');
const process = require('process');

childProcess.spawnSync(path.join(__dirname, 'npx'), ['proofread'], {
  stdio: 'inherit',
  env: { ...process.env, EXPO_NONINTERACTIVE: 1 },
});

